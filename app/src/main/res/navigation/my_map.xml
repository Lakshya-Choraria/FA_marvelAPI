<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_map"
    app:startDestination="@id/listFragment">

    <fragment
        android:id="@+id/listFragment"
        android:name="com.example.android.fa_marvelapi.Fragment.list.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_listFragment_to_incrementFragment"
            app:destination="@id/incrementFragment" />
    </fragment>
    <fragment
        android:id="@+id/incrementFragment"
        android:name="com.example.android.fa_marvelapi.Fragment.increment.incrementFragment"
        android:label="fragment_increment"
        tools:layout="@layout/fragment_increment" >
        <action
            android:id="@+id/action_incrementFragment_to_listFragment"
            app:destination="@id/listFragment" />
    </fragment>
</navigation>